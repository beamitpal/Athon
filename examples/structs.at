// Test struct feature

struct Point {
    x: int,
    y: int,
}

struct Person {
    age: int,
}

fn main() {
    // Create a Point
    let p = Point { x: 10, y: 20 };
    
    // Access members
    let x_val = p.x;
    let y_val = p.y;
    
    print("Point: x={}, y={}", x_val, y_val);
    
    // Create a Person
    let person = Person { age: 25 };
    print("Age: {}", person.age);
}
