fn main() {
    // Test break
    let i = 0;
    while i < 10 {
        if i == 5 {
            break;
        }
        print("i: {}", i);
        i = i + 1;
    }
    
    print("After break loop");
    
    // Test continue
    let j = 0;
    while j < 5 {
        j = j + 1;
        if j == 3 {
            continue;
        }
        print("j: {}", j);
    }
    
    print("After continue loop");
}
