{
  "Function": {
    "prefix": "fn",
    "body": [
      "fn ${1:name}(${2:params}) -> ${3:int} {",
      "\t${4:// function body}",
      "\treturn ${5:0};",
      "}"
    ],
    "description": "Function definition"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "fn main() {",
      "\t${1:// main code}",
      "}"
    ],
    "description": "Main function"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition} {",
      "\t${2:// code}",
      "}"
    ],
    "description": "If statement"
  },
  "If-Else Statement": {
    "prefix": "ifelse",
    "body": [
      "if ${1:condition} {",
      "\t${2:// if code}",
      "} else {",
      "\t${3:// else code}",
      "}"
    ],
    "description": "If-else statement"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition} {",
      "\t${2:// loop body}",
      "}"
    ],
    "description": "While loop"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:i} in ${2:0}..${3:10} {",
      "\t${4:// loop body}",
      "}"
    ],
    "description": "For loop"
  },
  "Match Statement": {
    "prefix": "match",
    "body": [
      "match ${1:value} {",
      "\t${2:pattern} => ${3:expression},",
      "\t_ => ${4:default},",
      "}"
    ],
    "description": "Match statement"
  },
  "Struct Definition": {
    "prefix": "struct",
    "body": [
      "struct ${1:Name} {",
      "\t${2:field}: ${3:int},",
      "}"
    ],
    "description": "Struct definition"
  },
  "Enum Definition": {
    "prefix": "enum",
    "body": [
      "enum ${1:Name} {",
      "\t${2:Variant1},",
      "\t${3:Variant2},",
      "}"
    ],
    "description": "Enum definition"
  },
  "Let Statement": {
    "prefix": "let",
    "body": "let ${1:name} = ${2:value};",
    "description": "Variable declaration"
  },
  "Print": {
    "prefix": "print",
    "body": "print(\"${1:message}\");",
    "description": "Print statement"
  },
  "Print with format": {
    "prefix": "printf",
    "body": "print(\"${1:format}\", ${2:args});",
    "description": "Print with formatting"
  },
  "Function (void)": {
    "prefix": "fnv",
    "body": [
      "fn ${1:name}(${2:params}) {",
      "\t${3:// function body}",
      "}"
    ],
    "description": "Function without return type"
  },
  "Array Literal": {
    "prefix": "array",
    "body": "let ${1:name} = [${2:1}, ${3:2}, ${4:3}];",
    "description": "Array literal"
  },
  "Array Index": {
    "prefix": "arridx",
    "body": "let ${1:element} = ${2:array}[${3:index}];",
    "description": "Array indexing"
  },
  "Struct Instance": {
    "prefix": "structinit",
    "body": [
      "let ${1:instance} = ${2:StructName} {",
      "\t${3:field}: ${4:value},",
      "};"
    ],
    "description": "Struct initialization"
  },
  "Enum Variant": {
    "prefix": "enumvar",
    "body": "let ${1:var} = ${2:EnumName}::${3:Variant};",
    "description": "Enum variant"
  },
  "Match with Blocks": {
    "prefix": "matchb",
    "body": [
      "match ${1:value} {",
      "\t${2:pattern} => {",
      "\t\t${3:// code}",
      "\t},",
      "\t_ => {",
      "\t\t${4:// default}",
      "\t},",
      "}"
    ],
    "description": "Match statement with block bodies"
  },
  "File Write": {
    "prefix": "fwrite",
    "body": [
      "let result = file_write(\"${1:filename}\", \"${2:content}\");",
      "if result == 1 {",
      "\t${3:// success}",
      "}"
    ],
    "description": "Write to file with error checking"
  },
  "File Read": {
    "prefix": "fread",
    "body": [
      "if file_exists(\"${1:filename}\") == 1 {",
      "\tlet content = file_read(\"${1:filename}\");",
      "\t${2:// use content}",
      "}"
    ],
    "description": "Read from file with existence check"
  },
  "Math Function": {
    "prefix": "math",
    "body": "let ${1:result} = ${2|abs,min,max,pow,sqrt,mod|}(${3:args});",
    "description": "Math function call"
  },
  "For Range Loop": {
    "prefix": "forrange",
    "body": [
      "for ${1:i} in ${2:start}..${3:end} {",
      "\t${4:// loop body}",
      "}"
    ],
    "description": "For loop with range"
  }
}
